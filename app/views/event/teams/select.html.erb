<div class="row">
  <div class="col-xs-4 col-xs-offset-4">
    <%= image_tag "#{current_user.profile.name.parameterize}.svg", class: 'img-fluid', style: 'max-height: 200px;' %>
  </div>
</div>
<% if current_user == @next_user %>
  <%= render 'selection' %>
<% else %>
  <%= render 'wait' %>
<% end %>
